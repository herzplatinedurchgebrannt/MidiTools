{
  "name": "angular-electron-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "start:dev": "electron dist/angular-electron-sample/shell.js",
    "start:prod": "electron dist/angular-electron-sample/main.js",
    "build": "ng build",
    "build:dev": "concurrently \"ng build -c=electron --delete-output-path=false --watch\" \"webpack --config webpack.electron.dev.config.js --watch\"",
    "build:prod": "ng build -c=electron --optimization --outputHashing=all --sourceMap=false --extractCss --namedChunks=false --extractLicenses --vendorChunk=false --buildOptimizer && webpack --config webpack.electron.prod.config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepackage": "cd dist/angular-electron-sample && npm install",
    "package": "electron-forge package",
    "analyze": "npx  @doyensec/electronegativity -i src/electron",
    "make": "electron-forge make"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/router": "~11.0.4",
    "bootstrap": "^5.0.1",
    "electron-log": "4.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "howler": "^2.2.2",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "webmidi": "^2.5.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@types/angular": "^1.8.1",
    "@types/node": "^12.11.1",
    "concurrently": "^5.3.0",
    "electron": "^11.1.0",
    "ts-loader": "^8.0.12",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "angular_electron_sample"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
